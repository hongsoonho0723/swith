<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.sportsmania.swith.mapper.userMapper">
    <insert id="join" parameterType="com.sportsmania.swith.DTO.userDTO">
        insert into user(userId,name,pwd,nickname,birthday,phone,email,joinType)
        values (#{userId},#{name},#{pwd},#{nickname},#{birthday},#{phone},#{email},#{joinType});
    </insert>
</mapper>